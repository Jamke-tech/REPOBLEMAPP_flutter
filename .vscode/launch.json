    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}",
            "args": ["--web-port", "5000"]
        },
        {
            "name": "Dart",
            "type": "dart",
            "request": "launch",
            "program": "bin/main.dart",
            "args": ["--web-port", "5000"]
        },

        {
            "name": "repoblemapp",
            "cwd": "repoblemapp",
            "request": "launch",
            "type": "dart",
            "args": ["--web-port", "5000"]
        }
    ]
}